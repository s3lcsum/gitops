# Watchtower SMTP Configuration
# Copy this file to watchtower.env and fill in your actual values

WATCHTOWER_NOTIFICATION_EMAIL_SERVER=your-smtp-server.com
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PORT=587
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_USER=your-email@domain.com
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PASSWORD=your-smtp-password
WATCHTOWER_NOTIFICATION_EMAIL_FROM=watchtower@yourdomain.com
WATCHTOWER_NOTIFICATION_EMAIL_TO=your-email@domain.com
