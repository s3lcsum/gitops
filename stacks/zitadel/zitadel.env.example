# Master key used by Zitadel (must be at least 32 characters)
ZITADEL_MASTERKEY=your-32-char-master-key-here-123456

# Database admin (must already exist in Postgres stack)
ZITADEL_DATABASE_POSTGRES_ADMIN_USERNAME=postgres
ZITADEL_DATABASE_POSTGRES_ADMIN_PASSWORD=change-me
ZITADEL_DATABASE_POSTGRES_ADMIN_SSL_MODE=disable

# App DB user (created by init if not exists)
ZITADEL_DATABASE_POSTGRES_USER_USERNAME=zitadel
ZITADEL_DATABASE_POSTGRES_USER_PASSWORD=change-me
ZITADEL_DATABASE_POSTGRES_USER_SSL_MODE=disable

# DB connection target
ZITADEL_DATABASE_POSTGRES_HOST=postgres
ZITADEL_DATABASE_POSTGRES_PORT=5432
ZITADEL_DATABASE_POSTGRES_DATABASE=zitadel
